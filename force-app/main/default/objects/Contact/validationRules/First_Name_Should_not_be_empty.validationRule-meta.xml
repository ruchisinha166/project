<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>First_Name_Should_not_be_empty</fullName>
    <active>true</active>
    <errorConditionFormula>ISBLANK( FirstName )</errorConditionFormula>
